<tr>
  <td><%= ticket.user.email %></td>
  <td><%= ticket.fio %></td>
  <td><%= ticket.passport_data %></td>
  <td><%= ticket.train.name %></td>
  <td><%= ticket.train.arrival_time(ticket.last_station_id) %></td>
  <td><%= ticket.train.departure_time(ticket.first_station_id) %></td>
  <td><%= link_to 'Show Details', ticket_path(ticket) %></td>
  <td><%= link_to 'Cancel Travel', ticket_path(ticket), method: :delete, data: { confirm: "Are you sure?" } %></td>
</tr>
